{
    "name": "vscode-comet",
    "displayName": "vscode-comet",
    "description": "Comet language syntax support",
    "publisher": "Cometlang",
    "repository": "https://github.com/comet/vscode-comet",
    "version": "0.2.2",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "comet",
            "aliases": ["Comet", "comet"],
            "extensions": [".cmt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "comet",
            "scopeName": "source.comet",
            "path": "./syntaxes/comet.tmLanguage.json"
        }]
    }
}